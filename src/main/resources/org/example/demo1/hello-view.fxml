<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="463.0" prefWidth="612.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23" fx:controller="org.example.demo1.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Label fx:id="studentID" text="StudentID">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Label>
      <Label fx:id="info" text="info" />
      <RadioButton mnemonicParsing="false" text="Car" />
      <RadioButton mnemonicParsing="false" text="Bus" />
      <RadioButton mnemonicParsing="false" text="Truck" />
      <Button mnemonicParsing="false" text="Calculate Tax" />
      <Label fx:id="result" text="Calculated Tax" />
   </children>
</VBox>
